function random_emoji --description "Get a random emoji"
  if count $argv >/dev/null
    set emojis $argv
  else
    set emojis $default_emojis
  end
  set emoji $emojis[(math (random)%(count $emojis)+1)] 
  echo -n " $emoji"
end